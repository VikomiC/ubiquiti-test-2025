{
  "name": "ubiquiti-test-2025",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && vite build",
    "eslint": "eslint src --ext \".js,.jsx,.ts,.tsx\"",
    "eslint-fix": "eslint src --ext \".js,.jsx,.ts,.tsx\" --fix",
    "stylelint": "npx stylelint \"src/**/*.scss\" --color",
    "stylelint-fix": "npx stylelint \"src/**/*.scss\" --color --fix",
    "lint:staged": "lint-staged",
    "lint:ts": "npm run eslint",
    "lint:format": "prettier --check './{src,cypress}/**/*.{ts,tsx}'",
    "lint:format:fix": "prettier --write './{src,cypress}/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "npm run stylelint",
    "lint": "tsc && npm run lint:ts && npm run lint:css",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": ["react-app", "react-app/jest"]
  },
  "dependencies": {
    "@tanstack/react-table": "8.21.2",
    "@tanstack/react-virtual": "3.13.2",
    "classnames": "2.5.1",
    "jotai": "2.12.1",
    "lucide-react": "0.477.0",
    "react": "19.0.0",
    "react-autosuggest": "10.1.0",
    "react-dom": "19.0.0",
    "react-resize-detector": "12.0.2",
    "react-router": "7.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "9.21.0",
    "@types/node": "22.13.8",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/parser": "8.25.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.21.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "16.0.0",
    "sass": "1.85.1",
    "stylelint": "16.15.0",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.11.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.25.0",
    "vite": "6.2.0",
    "vite-bundle-visualizer": "1.2.1",
    "vite-plugin-node-polyfills": "0.23.0",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
